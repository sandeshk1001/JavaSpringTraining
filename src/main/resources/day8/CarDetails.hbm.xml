<?xml version="1.0" encoding="UTF-8" ?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/orm/hbm">
    <class name="day8.CarDetails" table="CarDetails">
        <id type="int">
            <column name="id"></column>
        </id>
        <property name="name" column="name" type="string"/>
        <property name="speed" column="speed" type="double"></property>
        <property name="horsePower" column="horsePower" type="float"></property>
        <property name="maker" column="maker" type="char"></property>
        <property name="isHatchBack" column="isHatchBack" type="boolean"></property>
        <property name="carNumber" column="carNumber" type="long"></property>
    </class>
</hibernate-mapping>